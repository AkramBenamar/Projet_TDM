<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tp3">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera2" />
    <uses-feature android:name="android.hardware.camera2.autofocus" />

    <application
        android:name="com.example.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="DoClickS"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TP3">
        <activity android:name="com.example.dochomeActivity"></activity>
        <activity android:name="com.example.DoctorActivity" />
        <activity android:name="com.example.TraitementsActivity" />
        <activity android:name="com.example.RdvActivity" />
        <activity android:name="com.example.MainActivity" />
        <activity android:name="com.example.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>